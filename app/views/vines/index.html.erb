<h1>Our vines</h1>

<% @vines.each do |vine|%>
  <%= link_to vine_path(vine) do %>
  <div class="card">
    <ul>
      <li><%= vine.name %></li>
      <li><%= vine.picture %></li>
      <li><%= vine.location %></li>
      <li><%= vine.variety %></li>
      <li><%= vine.description %></li>
      <li><%= vine.price %></li>
    </ul>
  </div>
  <% end %>
<% end %>
